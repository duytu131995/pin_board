- @pins.each_slice(3) do |pins|
	.row
		- pins.each do |pin|
			.col-md-4
				.pin.panel.panel-default
					.image_wrapper
						= link_to (image_tag pin.image.url(:medium)), pin
					.panel-body
						%h2= link_to pin.title, pin
						%p
						Submitted by
						= pin.user.email
						%p
						%br/